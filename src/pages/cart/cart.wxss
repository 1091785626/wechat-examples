.v-cart image{
	flex:0 0 145rpx;
	height:145rpx;
}
.v-cart ._del{
	flex:0 0 110rpx;
	color:#fff;
}
.v-cart input[type="number"]{
	width: 220rpx;
	text-align:center;
}
.v-cart ._line{
	line-height: 2rpx;
	border-left: 130rpx solid #ddd;
	border-right: 130rpx solid #ddd;
}
.v-cart ._line .icon-cartadd{
	margin: 50rpx;
	font-size:50rpx;
}

